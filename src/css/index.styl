@import "./define"

.line
  width: 90%
  height: auto
  margin: auto
  text-align: center
  padding: 20px 0px 20px 0px

.title
  font-size: 20px
  font-weight: 500
  padding: 50px 30px 20px 30px

.row
  width: 100%
  height: 50px
  margin: auto
  display: flex
  flex-direction: row

.row:nth-child(even)
  background: #eeeeee;

.row1
