  height: 50px
  margin: auto
  display: flex
  line-height: 50px
  flex-direction: row
  background: #2196F3
  color: white
  .col
    font-size: 12pt
    width: 11%
    line-height: 50px
    padding: 0px 3px
    border-collapse: separate
    border-color: grey;
  .col1
    font-size: 12pt
    width: 17%
    line-height: 50px
    padding: 0px 3px
  .colc1
    font-size: 13pt
    line-height: 50px
    font-weight: 500
    width: 17%
    padding: 0px 3px
  .colc
    font-size: 13pt
    line-height: 50px
    font-weight: 500
    width: 11%
    padding: 0px 3px

.col
